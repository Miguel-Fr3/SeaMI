@model IEnumerable<SeaMI.Models.MonitoramentoAgua>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Sensores)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AmostraAgua)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dsMonitoramento)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Sensores.dsSensor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AmostraAgua.dsConcentracaoPlastico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dsMonitoramento)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.cdMonitoramentoAgua">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.cdMonitoramentoAgua">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.cdMonitoramentoAgua">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
