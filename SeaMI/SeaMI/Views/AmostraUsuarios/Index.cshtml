@model IEnumerable<SeaMI.Models.AmostraUsuario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Usuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AmostraAgua)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dsAmostra)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dtAmostra)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Usuario.dsArea)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AmostraAgua.dsConcentracaoPlastico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dsAmostra)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dtAmostra)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.cdAmostraUser">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.cdAmostraUser">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.cdAmostraUser">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
